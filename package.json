  "scripts": {
    "backend0atch": "TS_NODE_FILES=true nodemon --exec \"node --inspect -r ts-node/register -r tsconfig-paths/register src/index.ts\"",
    "backend:build": "yarn && rm -rf ./build && ttsc",
    "backend:test": "NODE_ENV=test jest --runInBand --forceExit",
    "backend:test:debug": "NODE_ENV=test node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "watch": "concurrently --names 'server,client' 'yarn backend:watch' 'cd client && yarn watch'",
    "deploy": "bash ./deploy.sh",
    "deploy:zip": "bash ./deploy.sh -z",
    "start": "NODE_ENV=production node ./deploy/build/index.js",
    "test": "node --inspect -r ts-node/register -r tsconfig-paths/register src/parser.ts"
  },
